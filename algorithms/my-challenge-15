function divisible(arr) {
    let newArr = [];
let count = 0

for(let i = 0; i < arr.length; i++) {
    if(arr[i] % 2 == 0 && arr[i] % 3 == 0 && arr[i] > 15) {
        newArr.push(arr[i])
        count++
    }
}
return count;
}

console.log(divisible([2,3,12,18,42,24]))
console.log(divisible([100,84,22,37]))
console.log(divisible([5,8,6,12]))