function isDivisibleByN(arr, n) { 
let newArr = [];

for (let i = 0; i < arr.length; i++) {                 //isDivisible should return [4, 6]
    if(arr[i] % n === 0) {
        newArr.push(arr[i])
    }
}
return new Set(newArr.sort())
}



