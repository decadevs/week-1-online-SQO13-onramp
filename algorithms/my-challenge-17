function sumOfDivisible(arr) {                   //Write a function that takes in an array and returns the sum of 
    let sum = 0;                                 //all the numbers divisible by 2 and 3 in the array

for(let i = 0; i < arr.length; i++) {
    if (arr[i] % 2 == 0 && arr[i] % 3 == 0) {
     sum += arr[i]
    }
}
return sum;
}

console.log(sumOfDivisible([2,5,7,3]))
console.log(sumOfDivisible([12,5,6,3]))
console.log(sumOfDivisible([6]))
console.log(sumOfDivisible([2,12,18]))